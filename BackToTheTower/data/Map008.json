rs":[6,1,false]},{"code":213,"indent":0,"parameters":[-1,1,false]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["！！！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"？？？"]},{"code":401,"indent":0,"parameters":["！！！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["啊……！"]},{"code":401,"indent":0,"parameters":["还好是人，吓死我了……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"？？？"]},{"code":401,"indent":0,"parameters":["哎呀！你把可可的脸都撞疼啦……！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["对不起，我们正忙着逃命，刚才没看到你。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["等等，你刚才说，你叫“可可”？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["对，我是唐可可，是结丘镇的见习天使。"]},{"code":401,"indent":0,"parameters":["二位是来拜访平安名神社的吗？为什么走的这么匆忙？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["啊，是堇提到的名字……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["可可，你认识平安名堇吗，神社的巫女？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["认识哦，我是堇的朋友。"]},{"code":401,"indent":0,"parameters":["你们找堇有什么事吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["堇在追杀我们……"]},{"code":401,"indent":0,"parameters":["因为我们意外看到了她变成具足虫的样子……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["怎么会……堇又变回具足虫了？"]},{"code":401,"indent":0,"parameters":["能告诉我发生了什么事吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["你应该知道了吧，刚才结丘镇发生了黑魔法暴乱。堇担心我和梨梨"]},{"code":401,"indent":0,"parameters":["感染了黑魔法，于是带我们去神社水晶进行魔力测试。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["而我……我确实受到了黑魔法感染，所以把神社水晶给污染了。"]},{"code":401,"indent":0,"parameters":["堇为了净化水晶，耗尽了魔力，倒下后就变成了具足虫……"]},{"code":401,"indent":0,"parameters":["等堇变回人形后，似乎是打算杀我们灭口……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["原来是这样啊……"]},{"code":401,"indent":0,"parameters":["那你的身体还好吗，受到黑魔法感染不及时治疗的话……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["啊啊，我没事，这一点感染对我来说只是小菜一碟。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["嗯，我已经听千砂都说了，是你们二位出手相助才平息了暴乱，想"]},{"code":401,"indent":0,"parameters":["必二位都是魔力高强的天使。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["不过堇没有即时给你治疗，却把个人恩怨放在第一位，确实是失职"]},{"code":401,"indent":0,"parameters":["了。请允许我替她向你们道歉。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["不过，我和梨梨已经被堇标记为了疑似感染的危险人物，如果她不"]},{"code":401,"indent":0,"parameters":["给我们撤销标记的话，我们就没法离开这里了。但是，我们还有很"]},{"code":401,"indent":0,"parameters":["重要的事情要去做。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["可可，既然你是堇的朋友，那应该很了解她吧？"]},{"code":401,"indent":0,"parameters":["你有办法让我们活着去见堇，给我们撤销标记吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["堇今天会变得这么生气，大概还是因为演唱会的事情吧……"]},{"code":401,"indent":0,"parameters":["不过，这个烂摊子毕竟是你们捅出来的，可可才不知道该怎么收拾。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["演唱会？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["啊，说漏嘴了！"]},{"code":401,"indent":0,"parameters":["不过被你们知道了应该也没什么关系……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["拜托了，可可！如果是你的话，肯定知道怎样才能让堇开心，对吧！"]},{"code":401,"indent":0,"parameters":["所以请帮我们一次！你想要什么回报都可以，哪怕是具足虫手办也"]},{"code":401,"indent":0,"parameters":["可以买给你！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"可可"]},{"code":401,"indent":0,"parameters":["哈？可可才不会收藏那种东西，被堇看到的话我可是会被她丢进海"]},{"code":401,"indent":0,"pa